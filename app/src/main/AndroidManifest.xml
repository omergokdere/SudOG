<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bingyan.net.demonsudoku">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DemonSudoku"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name=".SelectActivity"
            android:label="@string/title_activity_select" >
        </activity>
        <activity
            android:name=".DifficultyChoose"
            android:label="@string/title_activity_difficulty__choose" >
        </activity>
        <activity
            android:name=".LoadGame"
            android:label="@string/title_activity_load_game" >
        </activity>
        <activity
            android:name=".GameInfo"
            android:label="@string/title_activity_game_info" >
        </activity>
    </application>

</manifest>
